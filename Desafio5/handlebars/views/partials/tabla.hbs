<h2>Tabla de Registros</h2>
<hr>
<div>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Cantidad</th>
            <th>Precio</th>
        </tr>

        {{#each productos}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{cantidad}}</td>
                <td>{{precio}}</td>
            </tr>
        {{/each}}
    </table>
    <button onclick="window.location.href='/'">Regresar</button>
</div>